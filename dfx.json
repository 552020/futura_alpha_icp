{
  "canisters": {
    "backend": {
      "candid": "src/backend/backend.did",
      "package": "backend",
      "type": "rust",
      "declarations": {
        "output": "src/nextjs/src/ic/declarations/backend",
        "env_override": "process.env.NEXT_PUBLIC_CANISTER_ID_BACKEND"
      }
    },

    "canister_factory": {
      "candid": "src/canister_factory/canister_factory.did",
      "package": "canister_factory",
      "type": "rust",
      "declarations": {
        "output": "src/nextjs/src/ic/declarations/canister_factory",
        "env_override": "process.env.NEXT_PUBLIC_CANISTER_ID_FACTORY"
      }
    },

    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "frontend": {},
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "type": "custom",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "declarations": {
        "output": "src/nextjs/src/ic/declarations/internet_identity",
        "env_override": "process.env.NEXT_PUBLIC_CANISTER_ID_INTERNET_IDENTITY"
      }
    },

    "lab_backend": {
      "candid": "src/lab_backend/lab_backend.did",
      "package": "lab_backend",
      "type": "rust",
      "declarations": {
        "output": "src/lab_frontend/src/ic/declarations_lab/lab_backend"
      }
    },

    "lab_frontend": {
      "type": "assets",
      "source": ["src/lab_frontend/dist"],
      "build": ["bash", "-lc", "cd src/lab_frontend && pnpm install && pnpm run build"]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
